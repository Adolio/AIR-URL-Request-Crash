{
	"config": "air",
	"mainClass": "Main",
	"compilerOptions": {
		"advanced-telemetry": true,
		"output": "bin/AIR-Scaffold.swf",
		"library-path": [],
		"source-path": [
			"src"
		],
		"default-size": {
			"width": 1200,
			"height": 800
		}
	},
	"application": "application.xml",
